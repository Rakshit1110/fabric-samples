{
    "name": "cdms-chaincode",
    "version": "1.0.0",
    "description": "CDMS Contract for Chain of Custody Evidence Management",
    "main": "index.js",
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "lint": "eslint *.js lib/**/*.js",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive",
        "start": "fabric-chaincode-node start"
    },
    "engineStrict": false,
    "author": "CDMS Team",
    "license": "Apache-2.0",
    "dependencies": {
        "fabric-contract-api": "^2.5.0",
        "fabric-shim": "^2.5.0",
        "json-stringify-deterministic": "^1.0.0",
        "sort-keys-recursive": "^2.1.0",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "chai": "^4.4.1",
        "eslint": "^8.57.0",
        "mocha": "^10.4.0",
        "nyc": "^15.1.0",
        "sinon": "^18.0.0",
        "sinon-chai": "^3.7.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**",
            "index.js",
            ".eslintrc.js"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": false,
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
    }
}